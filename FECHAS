BEGIN
-- (Muestra los diferentes tipos de fechas, c/u)
DBMS_OUTPUT.PUT_LINE ('CURRENT DATE ES: ' ||CURRENT_DATE);
DBMS_OUTPUT.PUT_LINE ('CURRENT TIME STAMO ES:  ' ||CURRENT_TIMESTAMP);
DBMS_OUTPUT.PUT_LINE ('LOCALTIMESTAMP ES:  ' || LOCALTIMESTAMP);
DBMS_OUTPUT.PUT_LINE ('SYSDATE ES:    ' ||SYSDATE);
DBMS_OUTPUT.PUT_LINE ('SYSTIMESTAMP ES ' ||SYSTIMESTAMP);
END;
/
